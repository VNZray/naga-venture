{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.d.ts",
    "app/(screens)/*.tsx",
    "app/(screens)/**/*.tsx",
    "app/(tabs)/(home)/*.tsx",
    "app/(tabs)/(home)/**/*.tsx",
    "app/(screens)/_layout.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(tabs)/(home)/(shops)/(details)/[shopId].jsx"  ]
}
