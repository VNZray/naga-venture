{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.d.ts",
    "app/TouristApp/(screens)/*.tsx",
    "app/TouristApp/(screens)/**/*.tsx",
    "app/TouristApp/(tabs)/(home)/*.tsx",
    "app/TouristApp/(tabs)/(home)/**/*.tsx",
    "app/TouristApp/(screens)/_layout.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/TouristApp/(tabs)/(home)/(shops)/(details)/[shopId].jsx",
    "app/index.tsx",
    ".vscode/_layout.web.tsx",
    "app/BusinessApp/(admin)/feedback..webtsx"
  ]
}
